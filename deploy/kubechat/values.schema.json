{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "image": {
      "type": "object",
      "properties": {
        "registry": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        },
        "pullPolicy": {
          "type": "string"
        },
        "tag": {
          "type": "string"
        }
      },
      "required": [
        "registry",
        "repository",
        "pullPolicy",
        "tag"
      ]
    },
    "imagePullSecrets": {
      "type": "array",
      "items": {}
    },
    "nameOverride": {
      "type": "string"
    },
    "fullnameOverride": {
      "type": "string"
    },
    "efsId": {
      "type": "string"
    },
    "serviceAccount": {
      "type": "object",
      "properties": {
        "create": {
          "type": "boolean"
        },
        "annotations": {
          "type": "object"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "create",
        "annotations",
        "name"
      ]
    },
    "podAnnotations": {
      "type": "object"
    },
    "podSecurityContext": {
      "type": "object"
    },
    "securityContext": {
      "type": "object"
    },
    "service": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "port": {
          "type": "integer"
        }
      },
      "required": [
        "type",
        "port"
      ]
    },
    "resources": {
      "type": "object"
    },
    "nodeSelector": {
      "type": "object"
    },
    "tolerations": {
      "type": "array",
      "items": {}
    },
    "affinity": {
      "type": "object"
    },
    "django": {
      "type": "object",
      "properties": {
        "replicaCount": {
          "type": "integer"
        },
        "resources": {
          "type": "object"
        },
        "authEnabled": {
          "type": "boolean"
        },
        "auth0ClientId": {
          "type": "string",
          "minLength": 1
        },
        "auth0Domain": {
          "type": "string",
          "minLength": 1
        },
        "memoryRedisURL": {
          "type": "string",
          "minLength": 1
        },
        "modelServer": {
          "type": "string",
          "minLength": 1
        },
        "metaDBHost": {
          "type": "string",
          "minLength": 1
        },
        "metaDBPort": {
          "type": "integer",
          "minLength": 1
        },
        "metaDBDatabase": {
          "type": "string",
          "minLength": 1
        },
        "metaDBUser": {
          "type": "string",
          "minLength": 1
        },
        "metaDBPassword": {
          "type": "string",
          "minLength": 1
        },
        "vectorDBContext": {
          "type": "string",
          "minLength": 1
        },
        "vectorDBType": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "replicaCount",
        "resources",
        "authEnabled",
        "auth0ClientId",
        "auth0Domain",
        "memoryRedisURL",
        "modelServer",
        "metaDBHost",
        "metaDBPort",
        "metaDBDatabase",
        "metaDBUser",
        "metaDBPassword",
        "vectorDBContext",
        "vectorDBType"
      ]
    },
    "celery-worker": {
      "type": "object",
      "properties": {
        "replicaCount": {
          "type": "integer"
        },
        "brokerURL": {
          "type": "string"
        },
        "resources": {
          "type": "object"
        }
      },
      "required": [
        "replicaCount",
        "brokerURL",
        "resources"
      ]
    },
    "celery-beat": {
      "type": "object",
      "properties": {
        "replicaCount": {
          "type": "integer"
        },
        "resources": {
          "type": "object"
        }
      },
      "required": [
        "replicaCount",
        "resources"
      ]
    },
    "flower": {
      "type": "object",
      "properties": {
        "replicaCount": {
          "type": "integer"
        },
        "user": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "resources": {
          "type": "object"
        }
      },
      "required": [
        "replicaCount",
        "user",
        "password",
        "resources"
      ]
    }
  },
  "required": [
    "image",
    "imagePullSecrets",
    "nameOverride",
    "fullnameOverride",
    "efsId",
    "serviceAccount",
    "podAnnotations",
    "podSecurityContext",
    "securityContext",
    "service",
    "resources",
    "nodeSelector",
    "tolerations",
    "affinity",
    "django",
    "celery-worker",
    "celery-beat",
    "flower"
  ]
}